{
  "name": "ambassador-friday",
  "version": "1.0.0",
  "description": "Sample Cypress project to demonstrate how to run tests against different environments logging in with users with different roles.",
  "main": "cypress.config.js",
  "scripts": {
    "start": "node src/server.js",
    "start:server:and:test:local": "start-server-and-test start http://localhost:3000 test",
    "start:server:and:open:local": "start-server-and-test start http://localhost:3000 cy:open",
    "test": "npm run cy:run:local",
    "cy:open": "cypress open",
    "cy:open:test": "cypress open --config baseUrl=http://example.com --env environment=test",
    "cy:open:staging": "cypress open --config baseUrl=https://www.example.com --env environment=staging",
    "cy:open:prod": "cypress open --config baseUrl=https://example.com --env environment=prod",
    "cy:run:local": "cypress run",
    "cy:run:test": "cypress run --config baseUrl=http://example.com --env environment=test",
    "cy:run:staging": "cypress run --config baseUrl=https://www.example.com --env environment=staging",
    "cy:run:prod": "cypress run --config baseUrl=https://example.com --env environment=prod",
    "test:unit": "node --test cypress/support/setEnvironmentCredentials.test.js"
  },
  "keywords": [
    "cypress",
    "talking-about-testing",
    "testing"
  ],
  "author": "Walmyr <wlsf82@gmail.com> (https://walmyr.dev/)",
  "license": "MIT",
  "type": "commonjs",
  "devDependencies": {
    "cypress": "^15.9.0",
    "start-server-and-test": "^2.1.3"
  },
  "dependencies": {
    "express": "^5.2.1"
  }
}
